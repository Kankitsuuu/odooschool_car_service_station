<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="client_car_color_jet_black" model="car.service.client.car.color">
        <field name="name">Jet black</field>
        <field name="technical_code">RAL 9005</field>
        <field name="description">RAL 9005 Jet black color</field>
        <field name="is_metallic" eval="False"/>
    </record>

    <record id="client_car_color_pure_white" model="car.service.client.car.color">
        <field name="name">Pure white</field>
        <field name="technical_code">RAL 9010</field>
        <field name="description">RAL 9010 Pure white color</field>
        <field name="is_metallic" eval="False"/>
    </record>

    <record id="client_car_color_ultramarine_blue" model="car.service.client.car.color">
        <field name="name">Ultramarine blue</field>
        <field name="technical_code">RAL 5002</field>
        <field name="description">RAL 5002 Ultramarine blue color</field>
        <field name="is_metallic" eval="True"/>
    </record>

    <!-- Translation -->
    <record id="os_car_service.client_car_color_jet_black"
            model="car.service.client.car.color"
            context="{'lang': 'uk_UA'}">
        <field name="name">Чорний</field>
        <field name="description">RAL 9005 Чорний смоляний колір</field>
    </record>

    <record id="os_car_service.client_car_color_pure_white"
            model="car.service.client.car.color"
            context="{'lang': 'uk_UA'}">
        <field name="name">Чистий білий</field>
        <field name="description">RAL 9010 Чистий білий колір</field>
    </record>

    <record id="os_car_service.client_car_color_ultramarine_blue"
            model="car.service.client.car.color"
            context="{'lang': 'uk_UA'}">
        <field name="name">Синій ультрамарин</field>
        <field name="description">RAL 5002 Синій колір ультрамарин</field>
    </record>
    <!-- End Translation -->

</odoo>
